<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="clazz" extends="struts-default">
		<action name="clazzAction_*" class="clazzAction" method="{1}">
			<result name="listpage">/WEB-INF/jsps/clazz/clazzList.jsp</result>
			<result name="addpage">/WEB-INF/jsps/clazz/clazzAdd.jsp</result>
			<result name="addSuccess" type="redirectAction">clazzAction_listPage.action</result>
			<result name="input">/WEB-INF/jsps/clazz/clazzAdd.jsp</result>
		</action>
	</package>
</struts>